  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.0",
    "eslint": "^1.3.1",
    "espower-babel": "^3.3.0",
    "isparta": "^3.0.4",
    "mocha": "^2.2.5",
    "power-assert": "^1.0.0",
    "sinon": "^1.16.1",
    "web-audio-test-api": "^0.3.6"
  },
  "files": [
    "package.json",
    "README.md",
    "index.js",
    "lib"
  ],
  "license": "MIT",
  "main": "index.js",
